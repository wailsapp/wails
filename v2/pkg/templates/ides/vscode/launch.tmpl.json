{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Production {{.ProjectName}}",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/{{.PathToDesktopBinary}}",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Debug {{.ProjectName}}",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/{{.PathToDesktopBinary}}",
      "preLaunchTask": "build debug",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Dev {{.ProjectName}}",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/{{.PathToDesktopBinary}}",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}