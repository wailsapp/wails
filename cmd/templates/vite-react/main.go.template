package main

import (
  "github.com/wailsapp/wails"
  _ "embed"
)

func basic() string {
  return "World!"
}

//go:embed frontend/dist/{{.NPMProjectName}}.es.js
var js string

//go:embed frontend/src/main.css
var css string

func main() {

  app := wails.CreateApp(&wails.AppConfig{
    Width:  1024,
    Height: 768,
    Title:  "{{.Name}}",
    JS:     js,
    CSS:    css,
    Colour: "#131313",
  })
  app.Bind(basic)
  app.Run()
}
